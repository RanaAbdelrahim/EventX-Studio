PORT=5002
# MongoDB connection string - use 127.0.0.1 for local MongoDB
MONGO_URI=mongodb://127.0.0.1:27017/eventx
# Secret for JWT token generation
JWT_SECRET=change_this_to_a_secure_random_string
# Frontend URL for CORS (can be comma-separated list)
CLIENT_URL=http://localhost:5173
# Enable auto-seeding of demo data on server start
SEED_ON_START=true
# Debug level for seed script (1=verbose)
SEED_DEBUG=1
